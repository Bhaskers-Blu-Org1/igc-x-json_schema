{
  "type": "object",
  "$schema": "http://json-schema.org/schema#",
  "id": "https://github.com/OAI/OpenAPI-Specification/blob/master/versions/2.0.md#model-with-example",
  "title": "ModelWithExample",
  "properties": {
    "Model": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer",
          "format": "int64"
        },
        "name": {
          "type": "string"
        }
      },
      "required": [
        "name"
      ],
      "example": {
        "name": "Puma",
        "id": 1
      }
    }
  }
}
